{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
