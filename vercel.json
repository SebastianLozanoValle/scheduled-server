{
    "version": 2,
    "builds": [
        { "src": "./app.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/src/(.*)", "dest": "/app.js" },
        { "src": "/(.*)", "dest": "/app.js" },
        { "methods": ["GET", "POST", "PUT", "DELETE"] },
        { "headers": {
            "Access-Control-Allow-Origin": "*"
        }}
    ]
}